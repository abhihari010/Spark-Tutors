{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "firstwebsite/static/**/*",
        "use": "@vercel/static-build"
      },
      {
        "src": "firstwebsite/templates/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/firstwebsite/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }
  